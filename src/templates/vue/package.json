{
  "name": "swico-template-vue",
  "version": "1.0.0",
  "description": "Vue template for front-end framework Swico",
  "main": "index.js",
  "scripts": {
    "start": "npx husky init & swico start",
    "build": "swico build",
    "build:analyze": "cross-env ANALYZE=true swico build",
    "test": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,less,json,html,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/fanlaBoy/swico-template-vue.git"
  },
  "keywords": [
    "swico-template-vue",
    "swico",
    "typescript",
    "vue",
    "javascript",
    "webpack"
  ],
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "swico": "1.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/types": "7.23.9",
    "@commitlint/cli": "18.5.0",
    "@commitlint/config-conventional": "18.5.0",
    "@swc/core": "1.4.6",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vue/compiler-sfc": "3.4.14",
    "autoprefixer": "10.3.7",
    "chalk": "4.1.2",
    "chokidar": "3.6.0",
    "commander": "12.0.0",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "css-loader": "6.3.0",
    "esbuild-loader": "4.0.3",
    "eslint": "8.48.0",
    "eslint-plugin-vue": "9.19.2",
    "eslint-webpack-plugin": "4.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-loader": "5.0.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "9.0.10",
    "less": "4.1.2",
    "less-loader": "12.2.0",
    "lint-staged": "15.2.1",
    "mini-css-extract-plugin": "2.4.2",
    "ora": "5.4.1",
    "portfinder": "1.0.32",
    "postcss-loader": "8.1.0",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.2.5",
    "sass": "1.77.5",
    "sass-loader": "14.2.1",
    "style-loader": "3.3.0",
    "swc-loader": "0.2.6",
    "ts-node": "10.9.1",
    "typescript": "5.3.3",
    "vue-loader": "17.4.2",
    "vue-style-loader": "4.1.3",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.2",
    "webpack-merge": "5.9.0",
    "webpackbar": "6.0.0"
  }
}
