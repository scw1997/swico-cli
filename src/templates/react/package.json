{
  "name": "swico-template-react",
  "version": "1.0.0",
  "description": "React template for front-end framework Swico",
  "main": "index.js",
  "scripts": {
    "start": "swico start",
    "build": "swico build",
    "build:analyze": "cross-env ANALYZE=true swico build",
    "husky:init": "npx husky init",
    "test": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,less,json,html,md}": "prettier --write"
  },
  "keywords": [
    "swico-template-react",
    "swico",
    "typescript",
    "javascript",
    "react",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico.git"
  },
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "swico": "1.6.0"
  },
  "devDependencies": {
    "webpack": "5.94.0",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/node": "^22.13.1",
    "eslint": "8.48.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "prettier": "3.2.5",
    "husky": "9.0.10",
    "@commitlint/cli": "18.5.0",
    "@commitlint/config-conventional": "18.5.0",
    "lint-staged": "15.2.1",
    "typescript": "5.3.3",
    "cross-env": "7.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
