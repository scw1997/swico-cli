{
  "name": "swico-cli",
  "version": "2.0.0-beta.2",
  "description": "Cli for Front-End Framework Swico",
  "main": "index.js",
  "keywords": [
    "typescript",
    "javascript",
    "ecmascript",
    "react",
    "vue",
    "swico",
    "cli"
  ],
  "bin": {
    "swico": "./src/bin.js"
  },
  "scripts": {
    "compile": "tsc && ts-node ./src/task.ts",
    "publish:official": "npm run compile && cd ./dist && npm publish",
    "publish:beta": "npm run compile && cd ./dist && npm publish --tag=beta",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx} "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico-cli.git"
  },
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "12.0.0",
    "cross-spawn": "7.0.5",
    "download-git-repo": "3.0.2",
    "figlet": "1.7.0",
    "fs-extra": "11.2.0",
    "inquirer": "8.0.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "typescript-eslint": "8.30.1",
    "@eslint/js": "9.24.0",
    "@types/node": "22.13.10",
    "typescript": "5.8.3",
    "ts-node": "10.9.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
