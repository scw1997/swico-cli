{
  "name": "swico-cli",
  "version": "1.6.1",
  "description": "Cli for Front-End Framework Swico",
  "main": "index.js",
  "keywords": [
    "typescript",
    "javascript",
    "ecmascript",
    "react",
    "vue",
    "swico",
    "cli"
  ],
  "bin": {
    "swico": "./src/bin.js"
  },
  "scripts": {
    "compile": "tsc && ts-node ./src/task.ts",
    "publish:official": "npm run compile && cd ./dist && npm publish",
    "publish:beta": "npm run compile && cd ./dist && npm publish --tag=beta"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico-cli.git"
  },
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "12.0.0",
    "cross-spawn": "7.0.3",
    "download-git-repo": "3.0.2",
    "figlet": "1.7.0",
    "fs-extra": "11.2.0",
    "inquirer": "8.0.0",
    "ora": "5.4.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.48.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.1"
  }
}
